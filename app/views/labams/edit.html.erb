<h1>Editing labam</h1>

<% form_for(@labam) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :desc %><br />
    <%= f.text_area :desc %>
  </p>
  <p>
    <%= f.label :amount %><br />
    <%= f.text_field :amount %>
  </p>
   <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @labam %> |
<%= link_to 'Back', labams_path %>
